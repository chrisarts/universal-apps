{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".expo/**"
      ]
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
